<odoo>
    <record id="ncr_type_action" model="ir.actions.act_window">
        <field name="name">NCR Type</field>
        <field name="res_model">x.ncr.type</field>
        <field name="view_mode">tree</field>
    </record>
    <record id="ncr_type_view_tree" model="ir.ui.view">
        <field name="name">x.ncr.type.tree</field>
        <field name="model">x.ncr.type</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>
