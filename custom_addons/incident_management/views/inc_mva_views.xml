<odoo>
    <record id="inc_mva_view_tree" model="ir.ui.view">
        <field name="name">inc.mva.tree</field>
        <field name="model">x.inc.mva.record</field>
        <field name="arch" type="xml">
            <tree string="MVA" editable="bottom">
                <field name="id"/>
                <field name="mva_classification"/>
                <field name="location"/>
                <field name="damage"/>
                <field name="description"/>
                <field name="immediate_action"/>
            </tree>

        </field>
    </record>
</odoo>