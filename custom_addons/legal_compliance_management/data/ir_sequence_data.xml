<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for Legal register -->
        <record id="ir_sequence_environment" model="ir.sequence">
            <field name="name">Environment System Regulations</field>
            <field name="code">x.legal.environment.regulation</field>
            <field name="prefix">LR/ENV</field>
            <field name="padding">2</field>
            <field name="number_increment">1</field>
        </record>
        <record id="ir_sequence_saso" model="ir.sequence">
            <field name="name">SASO</field>
            <field name="code">x.legal.saso</field>
            <field name="prefix">LR/SASO</field>
            <field name="padding">2</field>
            <field name="number_increment">1</field>
        </record>
        <record id="ir_sequence_mhrs" model="ir.sequence">
            <field name="name">MHRS</field>
            <field name="code">x.legal.mhrs</field>
            <field name="prefix">LR/MHRS</field>
            <field name="padding">2</field>
            <field name="number_increment">1</field>
        </record>
        <record id="ir_sequence_other_regulations" model="ir.sequence">
            <field name="name">Other Regulations</field>
            <field name="code">x.legal.other</field>
            <field name="prefix">LR/other</field>
            <field name="padding">2</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
